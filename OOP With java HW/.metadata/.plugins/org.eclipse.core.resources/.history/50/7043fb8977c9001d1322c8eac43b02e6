package examples;

public class RecursivelyRemoveAdjacentDuplicates {
	String rremove(String s) {
		String[] arr=s.split("");
		int count=1;
		for(int i=0;i<arr.length-1;i++) {
			if(arr[i]==arr[i+1]) {
				count++;
				String str="";
				while(count>0) {
					
				}
			}
		}
	}
